  "scripts": {
    "dev": "rollup -c -w",
    "build": "node build/build.js",
    "lint": "eslint src build test",
    "test:server": "node ./node_modules/browser-sync/bin/browser-sync start --server ./",
    "test:e2e": "node test/e2e/runner.js",
